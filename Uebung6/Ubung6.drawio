        <mxCell id="1ledMZv2JU0vycFWsOgZ-15" value="NullPointer Collaboration Portal" style="rounded=0;whiteSpace=wrap;html=1;fillColor=none;align=center;verticalAlign=top;" vertex="1" parent="1">
